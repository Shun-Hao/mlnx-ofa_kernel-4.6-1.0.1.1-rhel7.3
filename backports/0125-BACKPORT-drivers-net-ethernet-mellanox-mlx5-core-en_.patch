+				&ipv6h->daddr, ntohs(th->dest),
+				netdev->ifindex, 0);
+#endif
 #endif
 	}
 	if (!sk || sk->sk_state == TCP_TIME_WAIT) {
@@ -381,3 +389,5 @@ void mlx5e_tls_handle_rx_skb(struct net_device *netdev, struct sk_buff *skb,
 	remove_metadata_hdr(skb);
